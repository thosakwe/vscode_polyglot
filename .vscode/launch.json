{
	"version": "0.1.0",
	"configurations": [
		{
			"name": "Launch Extension",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceRoot}/build/web"
			],
            "preLaunchTask": "grind",
			"stopOnEntry": false,
			"sourceMaps": true
		}
	]
}